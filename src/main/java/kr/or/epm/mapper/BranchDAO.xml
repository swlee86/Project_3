<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.epm.DAO.BranchDAO">

	<select id="selectBranch" resultType="kr.or.epm.VO.Branch">
		select branch_name from branch where his_check=1
	</select>
	
	<select id="selectChooseBranch" parameterType="String" resultType="kr.or.epm.VO.Branch">
		select * from branch where his_check=1 and branch_name=#{selectBranchName}
	</select>
	
	<insert id="addBranch" parameterType="kr.or.epm.VO.Branch">
		insert into branch values (13, #{branch_name}, #{postcode},#{addr},#{addr_detail},null,1,0)
	</insert>
	
</mapper>